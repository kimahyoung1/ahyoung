package sec01.exam01;

public class StudentExam {

	int a= 10; // 필드는 공통으로 쓰는 경우 클래스 안쪽으로 뺸다
	
	public static void main(String[] args) {
		
		// 우리가 만든 클래스를 불러오는 작업 
		Student 	s1 = new Student ( ); //새로운 Student 객체를 생성해서 s1에 담기
		
		
		Student 	s2 = new Student ( );
		
		Student 	s3 = null; //변수만 만들고 연결은 없음
		
		// == 주소값만 입력(steak 영역)
		s3= s1;
		System.out.println(s3==s1); //  같은 영역이라서 true가 나옴
		System.out.println(s1==s2); // 전혀다른 곳이라서  false가 나옴
		s2 = null;
		
		
		
 	}

}
